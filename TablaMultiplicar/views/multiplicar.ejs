<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabla de Multiplicar del <%= numero %></title>
    <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
    <div class="container">
        <h1>Tabla de multiplicar del <%= numero %></h1>
        <table>
            <thead>
                <tr>
                    <th>Multiplicando</th>
                    <th>Resultado</th>
                </tr>
            </thead>
            <tbody>
                <% multiplicacion.forEach(function(item) { %>
                    <tr>
                        <td><%= numero %> x <%= item.multiplicando %> </td>
                        <td><%= item.resultado %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
</body>
</html>
