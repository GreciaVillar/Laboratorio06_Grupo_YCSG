    <style>
        .testimonial-section {
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
            padding: 50px 0;
        }
    
        .section-title {
            font-size: 28px;
            font-weight: bold;
            margin-bottom: 20px;
            text-align: center;
        }
    
        .section-subtitle {
            font-size: 24px;
            margin-bottom: 40px;
            text-align: center;
        }
    
        .testimonial-carousel {
            display: flex;
            overflow: hidden;
            width: 80%; /* Ancho del carrusel */
            justify-content: center;
        }
    
        .testimonial-card {
            flex: 0 0 auto;
            width: 40%; /* Ancho de cada tarjeta de testimonio */
            padding: 20px;
            margin-right: 30px;
            border-radius: 20px;
            background-color:rgb(214, 233, 250);
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
            transition: transform 0.3s ease-in-out;
        }
    
        .testimonial-card:last-child {
            margin-right: 0;
        }
    
        .testimonial-card:hover {
            transform: scale(1.05);
        }
    
        .testimonial-content {
            text-align: center;
        }
    
        .testimonial-message {
            font-size: 16px;
            margin-bottom: 10px;
        }
    
        .testimonial-name {
            font-size: 18px;
            font-weight: bold;
            margin-bottom: 5px;
        }
    
        .testimonial-email {
            font-size: 14px;
            color: #666;
        }
    
        .no-testimonial-message {
            font-size: 18px;
            color: #666;
            text-align: center;
            margin-top: 30px;
        }
    </style>
    
    <script>
        let isDragging = false;
        let startPosition = 0;
        let currentTranslate = 0;
        let currentIndex = 0;
        const carousel = document.getElementById('testimonialCarousel');
        const cards = document.querySelectorAll('.testimonial-card');
    
        cards.forEach((card, index) => {
            card.addEventListener('mousedown', e => {
                isDragging = true;
                startPosition = e.clientX;
                currentTranslate = carousel.scrollLeft;
            });
    
            card.addEventListener('mouseup', () => {
                isDragging = false;
                const scrollAmount = carousel.clientWidth;
                if (Math.abs(startPosition - carousel.scrollLeft) > 50) {
                    if (startPosition < carousel.scrollLeft) {
                        currentIndex++;
                    } else {
                        currentIndex--;
                    }
                    carousel.scrollTo({
                        left: currentIndex * scrollAmount,
                        behavior: 'smooth'
                    });
                } else {
                    carousel.scrollTo({
                        left: currentIndex * scrollAmount,
                        behavior: 'smooth'
                    });
                }
            });
    
            card.addEventListener('mousemove', e => {
                if (isDragging) {
                    carousel.scrollLeft = currentTranslate + (startPosition - e.clientX);
                }
            });
        });
    
        // Código para hacer que el carrusel funcione automáticamente
        setInterval(() => {
            currentIndex = (currentIndex + 1) % cards.length;
            const scrollAmount = carousel.clientWidth;
            carousel.scrollTo({
                left: currentIndex * scrollAmount,
                behavior: 'smooth'
            });
        }, 4000);
    </script>
    




